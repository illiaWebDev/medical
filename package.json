  "dependencies": {
    "@docusaurus/core": "2.4.3",
    "@docusaurus/preset-classic": "2.4.3",
    "@mdx-js/react": "1.6.22",
    "clsx": "1.2.1",
    "prism-react-renderer": "1.3.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "6.0.8"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.4.3",
    "@tsconfig/docusaurus": "1.0.5",
    "@types/eslint": "8.4.10",
    "@types/node": "16.18.23",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
